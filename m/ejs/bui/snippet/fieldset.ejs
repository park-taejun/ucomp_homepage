<!DOCTYPE html>
<html lang="ko">
<head>
<%-include('../_templates/_data.header.front.ejs', {
	pageStyle: 'bui.page.snippet.css',
	pageJavascript: ''
}) %>
</head>
<body>
<div id="wrap">
	<!-- page -->
	<div id="page">
<%-include('../_templates/_header.snippet.ejs') %>
		<!-- page-body -->
		<div class="page-body" id="local">
			<!-- local-head -->
			<div class="local-head">
				<h2 class="local-title" id="localTitle"><span class="local-name">snippet</span></h2>
			</div>
			<!-- //local-head -->
			<!-- local-body -->
			<div class="local-body" id="content">
				<!-- content-head -->
				<div class="content-head"><%- include('../_mixin/_title.content.ejs', {name: 'fieldset'}) %></div>
				<!-- //content-head -->
				<!-- content-body -->
				<div class="content-body">
					<!-- section : Button System-->
					<div class="section">
						<div class="section-head"><%- include('../_mixin/_title.section.ejs', {name: 'submit form'}) %></div>
						<div class="section-body">
							<!-- subsection -->
							<div class="subsection">
								<div class="subsection-head"><%- include('../_mixin/_title.subsection.ejs', {name: 'HTML Structure'}) %></div>
								<div class="subsection-body">
									<pre class="highlight-js">
<code class="html">&lt;!-- submit-form --&gt;
&lt;fieldset class="submit-form"&gt;
	&lt;legend&gt;fieldset name&lt;/legend&gt;
	&lt;div class="form-list"&gt;
		&lt;div class="form-item"&gt;
			&lt;div class="form-wrap"&gt;
				&lt;div class="form-head"&gt;&lt;label class="form-name" for="form_id"&gt;form name&lt;/label&gt;&lt;/div&gt;
				&lt;div class="form-body"&gt;
					&lt;div class="form-area"&gt;

					&lt;/div&gt;
				&lt;/div&gt;
			&lt;/div&gt;
		&lt;/div&gt;
	&lt;/div&gt;
&lt;/fieldset&gt;
&lt;!-- //submit-form --&gt;</code></pre>
								</div>
							</div>
							<!-- //subsection -->
							<!-- subsection -->
							<div class="subsection">
								<div class="subsection-head"><%- include('../_mixin/_title.subsection.ejs', {name: 'Class Options'}) %></div>
								<div class="subsection-body">
									<!-- data-table -->
									<div class="data-table module-bui type-a">
										<table>
											<colgroup>
												<col style="width: 128rem;">
												<col>
											</colgroup>
											<tbody>
												<tr>
													<th scope="row">module</th>
													<td>module-*</td>
												</tr>
												<tr>
													<th scope="row">style</th>
													<td>style-*</td>
												</tr>
												<tr>
													<th scope="row">type</th>
													<td>type-*</td>
												</tr>
												<tr>
													<th scope="row">outline</th>
													<td>xx-small, x-small, small, medium, large, x-large, xx-large, xx-large</td>
												</tr>
												<tr>
													<th scope="row">symbols</th>
													<td>attr-*</td>
												</tr>
											</tbody>
										</table>
									</div>
									<!-- //data-table -->
								</div>
							</div>
							<!-- //subsection -->
							<!-- subsection -->
							<div class="subsection">
								<div class="subsection-head"><%- include('../_mixin/_title.subsection.ejs', {name: 'demos'}) %></div>
								<div class="subsection-body">

									<!--subsubsection-->
									<div class="subsubsection">
										<div class="subsubsection-head"><%- include('../_mixin/_title.subsubsection.ejs', {name: '연차 등록'}) %></div>
										<div class="subsubsection-body">
											<!-- submit-form -->
											<fieldset class="submit-form module-a type-a medium">
												<legend>연차 등록 입력 서식</legend>
												<div class="form-list">
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="periodLeave">일시</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="text" class="form-elem" id="periodLeave" placeholder="일시를 입력해주세요." readonly="readonly" value="11, 16, 18, 21일">
																	</span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><span class="form-name">업무연락</span></div>
															<div class="form-body">
																<div class="form-area">
																	<label class="form checkbox module-b style-b type-line normal-04 medium flex"><input type="checkbox" class="form-elem" checked="checked"> <span class="form-text">전화</span></label>
																	<label class="form checkbox module-b style-b type-line normal-04 medium flex"><input type="checkbox" class="form-elem"> <span class="form-text">팀즈</span></label>
																	<label class="form checkbox module-b style-b type-line normal-04 medium flex"><input type="checkbox" class="form-elem"> <span class="form-text">이메일</span></label>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="phoneNumbdr">연락처</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="tel" class="form-elem typed" id="phoneNumbdr" placeholder="연락처를 입력해주세요." value="010-2729-5982" onfocus="buiFormCancel(this);" onmousemove="buiFormCancel(this);" style="padding-right: 46rem;">
																	<span class="form-func"><span class="form-cancel"></span></span></span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="todayTask">업무내역</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textarea module-b style-b type-line normal-04 medium flex">
																		<textarea class="form-elem" id="todayTask" rows="4" placeholder="업무 내역을 입력해 주세요."></textarea>
																	</span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="requestReason">사유</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="text" class="form-elem typed" id="requestReason" placeholder="" value="UCOMP 인트라넷" onfocus="buiFormCancel(this);" onmousemove="buiFormCancel(this);" style="padding-right: 46rem;">
																	<span class="form-func"><span class="form-cancel"></span></span></span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="approver">승인위치</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form select module-b style-b type-line normal-04 medium flex">
																		<select class="form-elem" id="approver">
																			<option>조완섭[수석]_UX팀</option>
																		</select>
																	</span>
																</div>
															</div>
														</div>
													</div>
												</div>
											</fieldset>
											<!-- //submit-form -->
										</div>
									</div>
									<!--//subsubsection-->
									<!--subsubsection-->
									<div class="subsubsection">
										<div class="subsubsection-head"><%- include('../_mixin/_title.subsubsection.ejs', {name: '지출결의'}) %></div>
										<div class="subsubsection-body">
											<!-- submit-form -->
											<fieldset class="submit-form module-a type-a medium">
												<legend>지출결의 입력 서식</legend>
												<div class="form-list">
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="requesterName">신청자</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="text" class="form-elem" id="requesterName" placeholder="" value="이지혜" readonly="readonly" onfocus="buiFormCancel(this);" onmousemove="buiFormCancel(this);">
																	</span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="requesterDepartment">본부</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="text" class="form-elem" id="requesterDepartment" placeholder="" value="서비스 운영 본부" readonly="readonly" onfocus="buiFormCancel(this);" onmousemove="buiFormCancel(this);">
																	</span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="requesterTeam">부서</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="text" class="form-elem" id="requesterTeam" placeholder="" value="UX팀" readonly="readonly" onfocus="buiFormCancel(this);" onmousemove="buiFormCancel(this);">
																	</span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="requestTitle">제목</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="text" class="form-elem typed" id="requestTitle" placeholder="" value="02월 지출결의_이지혜" onfocus="buiFormCancel(this);" onmousemove="buiFormCancel(this);" style="padding-right: 46rem;">
																	<span class="form-func"><span class="form-cancel"></span></span></span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="approver">승인위치</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form select module-b style-b type-line normal-04 medium flex">
																		<select class="form-elem" id="approver">
																			<option>선택</option>
																			<option>조완섭[수석]_UX팀</option>
																		</select>
																	</span>
																</div>
															</div>
														</div>
													</div>
												</div>
											</fieldset>
											<!-- //submit-form -->
										</div>
									</div>
									<!--//subsubsection-->
									<!--subsubsection-->
									<div class="subsubsection">
										<div class="subsubsection-head"><%- include('../_mixin/_title.subsubsection.ejs', {name: '지출결의 상세내역'}) %></div>
										<div class="subsubsection-body">
											<!-- submit-form -->
											<fieldset class="submit-form module-a type-a medium">
												<legend>지출내역 입력 서식</legend>
												<div class="form-list">
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="expensesDate">날짜</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form date module-b style-b type-line normal-04 medium flex">
																		<input type="date" class="form-elem" id="expensesDate" placeholder="날짜를 선택해주세요." value="2022-02-11">
																	</span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="expensesType">지출타입</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form select module-b style-b type-line normal-04 medium flex">
																		<select class="form-elem" id="expensesType">
																			<option>선택</option>
																			<option>교통비</option>
																			<option>석식대</option>
																			<option>회식비</option>
																			<option>기타</option>
																		</select>
																	</span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="expensesHistory">상세내역</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="text" class="form-elem typed" id="expensesHistory" placeholder="지출 상세내역을 입력해주세요." value="12시 지나 퇴근이라 택시탐" onfocus="buiFormCancel(this);" onmousemove="buiFormCancel(this);" style="padding-right: 46rem;">
																	<span class="form-func"><span class="form-cancel"></span></span></span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="expensesProjectName">프로젝트</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="text" class="form-elem typed" id="expensesProjectName" placeholder="프로젝트명을 입력해주세요." value="UCOMP 인트라넷" onfocus="buiFormCancel(this);" onmousemove="buiFormCancel(this);" style="padding-right: 46rem;">
																	<span class="form-func"><span class="form-cancel"></span></span></span>
																</div>
															</div>
														</div>
													</div>
													<div class="form-item">
														<div class="form-wrap">
															<div class="form-head"><label class="form-name" for="expensesAmount">금액</label></div>
															<div class="form-body">
																<div class="form-area">
																	<span class="form textfield module-b style-b type-line normal-04 medium flex">
																		<input type="text" class="form-elem typed" id="expensesAmount" placeholder="금액을 입력해주세요." value="11,000" pattern="^[0-9]+$" onfocus="buiFormCancel(this);" onmousemove="buiFormCancel(this);" style="padding-right: 46rem;">
																	<span class="form-func"><span class="form-cancel"></span></span></span>
																</div>
															</div>
														</div>
													</div>
												</div>
											</fieldset>
											<!-- //submit-form -->
										</div>
									</div>
									<!--//subsubsection-->
								</div>
							</div>
							<!-- //subsection -->
						</div>
					</div>
					<!--//section -->
				</div>
				<!-- //content-body -->
			</div>
			<!-- //local-body -->
		</div>
		<!-- //page-body -->
<%-include('../_templates/_footer.front.ejs') %>
	</div>
	<!-- //page -->
</div>
</body>
</html>